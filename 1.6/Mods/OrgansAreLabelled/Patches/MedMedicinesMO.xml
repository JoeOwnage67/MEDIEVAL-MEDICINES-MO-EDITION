						Natural:
						<color>(190,190,190)</color>
						<colorTwo>(145,170,170)</colorTwo>
						
						Medieval Basic:
						<color>(181,98,31)</color>
                        <colorTwo>(133,97,67)</colorTwo>

                        Medieval Artificial:
                        <color>(181,98,31)</color>
                        <colorTwo>(120,105,92)</colorTwo>

                        Medieval Mechanical:
                        <color>(181,98,31)</color>
                        <colorTwo>(156,132,39)</colorTwo>

                        Medieval Paresian:
                        <color>(181,98,31)</color>
            			<colorTwo>(161,16,194)</colorTwo>
						
						Simple:
						<color>(154,124,104)</color>
						
						Bionic:
						<color>(145,170,170)</color>
