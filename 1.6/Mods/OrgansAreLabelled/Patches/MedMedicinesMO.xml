    <!-- Medieval Basic -->

    <!-- remove graphicdata from parent -->
	<Operation Class="PatchOperationConditional">
		<xpath>/Defs/ThingDef[defName="Basic"]/graphicData</xpath>
		<match Class="PatchOperationRemove">
			<xpath>/Defs/ThingDef[defName="Basic"]/graphicData</xpath>
		</match>
	</Operation>

    <!-- add graphic data to each body part def -->
    <!-- IronArm -->
	<Operation Class="PatchOperationAdd">
	<xpath>/Defs/ThingDef[defName="IronArm"]</xpath>
	<value>
		<graphicData Inherit="False">
			<texPath>Things/Organs/arm</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<color>(100,70,30)</color>
            <colorTwo>(0,102,0)</colorTwo>
			<ignoreThingDrawColor>true</ignoreThingDrawColor>
			<drawSize>0.90</drawSize>
		</graphicData>
	</value>
	</Operation>

